{
  "kind": "collectionType",
  "collectionName": "customers",
  "info": {
    "singularName": "customer",
    "pluralName": "customers",
    "displayName": "Customer"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "identifier": {
      "type": "uid",
      "required": true
    },
    "first_name": {
      "type": "string",
      "required": true
    },
    "last_name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "password": {
      "type": "password",
      "required": true
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "male",
        "female",
        "trans"
      ]
    },
    "identifier_adress": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::adress.adress",
      "inversedBy": "customer_id"
    }
  }
}
